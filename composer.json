{
    "name": "i-neop/laravel-file-upload",
    "description": "laravel file upload is package that provides to you upload single file or multi files",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0.2",
        "illuminate/contracts": "^9.0",
        "intervention/image": "^2.7",
        "spatie/laravel-medialibrary": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "INeop\\FileUpload\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "INeop\\FileUpload\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "IbrahimNeop2022",
            "email": "i.ismail@neopplanet.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "INeop\\FileUpload\\Providers\\FileUploadServiceProvider"
            ],
            "aliases": {
                "MediaUpload": "INeop\\FileUpload\\Classes\\Facades\\MediaUpload",
                "FileUpload":  "INeop\\FileUpload\\Classes\\Facades\\FileUpload"
            }
        }
    }
}
